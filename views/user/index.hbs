<script>
  function nonEmpty() {
    const input = document.querySelectorAll("input");
    var flag = 0;
    for (let i = 0; i <= 10; i++) {
      if (input[i].value.length == 0) {
        flag = 1;
      } else {
        flag = 0;
      }

      if (flag == 1) {
        break;
      }
    }
    return flag;
  }
  const repeatChecker = (e) => {
    var n1 = document.getElementById('delegateonemail'),
      n2 = document.getElementById('delegatetwomail'),
      n3 = document.getElementById('delegatethreemail');
    var p1 = document.getElementById("delegateonephno"),
      p2 = document.getElementById('delegatetwophno'),
      p3 = document.getElementById('delegatethreephno'),
      btn = document.getElementById("submit-btn");
    console.log(btn.disabled);

    (((n1.value == n2.value) || (n2.value == n3.value) || (n3.value == n1.value)) && ((p1.value == p2.value) || (p2.value == p3.value) || (p3.value == p1.value)) && nonEmpty()!=1) ? btn.disabled = true : btn.disabled = false;
  }
  const input = document.querySelectorAll('input');
  input.forEach((inp) => inp.addEventListener('change', function () {
    (inp.length != 0) ? repeatChecker(inp) : console.log(inp);
  }));

</script>
<script>
      document.forms["summit-form"].addEventListener("submit", async (event) => {
